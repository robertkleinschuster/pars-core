{
  "name": "pars/pars-core",
  "description": "",
  "license": "BSD-3-Clause",
  "keywords": [
    "pars"
  ],
  "support": {
    "docs": "https://docs.parsphp.org/pars-core/",
    "issues": "https://github.com/pars/pars-core/issues",
    "source": "https://github.com/pars/pars-core",
    "rss": "https://github.com/pars/pars-core/releases.atom",
    "forum": "https://discourse.parsphp.org/"
  },
  "require": {
    "php": "^7.4",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-gd": "*",
    "composer/composer": "^2.0",
    "laminas/laminas-stdlib": "^3.2.1",
    "laminas/laminas-diactoros": "^2.3.0",
    "laminas/laminas-servicemanager": "^3.4",
    "laminas/laminas-eventmanager": "^3.3",
    "laminas/laminas-log": "^2.12",
    "laminas/laminas-db": "^2.11",
    "laminas/laminas-hydrator": "^3.1",
    "laminas/laminas-cache": "^2.9",
    "laminas/laminas-i18n": "^2.10",
    "laminas/laminas-mail": "^2.12",
    "laminas/laminas-config-aggregator": "^1.2",
    "laminas/laminas-serializer": "^2.9",
    "laminas/laminas-zendframework-bridge": "^1.0",
    "mezzio/mezzio": "^3.2.1",
    "mezzio/mezzio-fastroute": "^3.0.3",
    "mezzio/mezzio-platesrenderer": "^2.2",
    "mezzio/mezzio-helpers": "^5.3",
    "mezzio/mezzio-authentication": "^1.1",
    "mezzio/mezzio-authentication-session": "^1.0",
    "mezzio/mezzio-session": "^1.3",
    "mezzio/mezzio-session-cache": "^1.4",
    "mezzio/mezzio-csrf": "^1.0",
    "mezzio/mezzio-flash": "^1.0",
    "pars/pars-helpers": "dev-master",
    "pars/pars-models": "dev-master",
    "league/flysystem": "^1.1",
    "league/glide": "^1.6",
    "league/uri": "^6.4.0",
    "league/oauth2-client": "^2.6",
    "lcobucci/jwt": "^4.1",
    "cache/filesystem-adapter": "^1.0",
    "cocur/slugify": "^4.0",
    "pars/pars-beans": "dev-master",
    "matthiasmullie/minify": "^1.3",
    "scssphp/scssphp": "^1.3",
    "twbs/bootstrap": "4.6.0",
    "padaliyajay/php-autoprefixer": "^1.3",
    "wikimedia/composer-merge-plugin": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.5",
    "laminas/laminas-development-mode": "^3.2",
    "mezzio/mezzio-tooling": "^1.3",
    "phpspec/prophecy": "^1.10.3",
    "phpspec/prophecy-phpunit": "^2.0",
    "roave/security-advisories": "dev-master",
    "filp/whoops": "^2.7.1"
  },
  "autoload": {
    "psr-4": {
      "Pars\\Core\\": "src//"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ParsTest\\Core\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "merge-plugin": {
      "include": [
        "vendor/pars/pars-core/composer.json"
      ],
      "require": [
      ],
      "recurse": true,
      "replace": false,
      "ignore-duplicates": false,
      "merge-dev": true,
      "merge-extra": true,
      "merge-extra-deep": false,
      "merge-scripts": true
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "classmap-authoritative": true
  },
  "scripts": {
    "serve": "php -S 0.0.0.0:8080 -t public/",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "post-update-cmd": [
      "Pars\\Core\\Deployment\\UpdateHandler::postUpdate"
    ],
    "post-install-cmd": [
      "Pars\\Core\\Deployment\\UpdateHandler::postInstall"
    ],
    "post-package-install": [
      "Pars\\Core\\Deployment\\UpdateHandler::postPackageInstall"
    ],
    "post-package-update": [
      "Pars\\Core\\Deployment\\UpdateHandler::postPackageUpdate"
    ],
    "post-autoload-dump": [
      "Pars\\Core\\Deployment\\UpdateHandler::postAutoloadDump"
    ]
  }
}
